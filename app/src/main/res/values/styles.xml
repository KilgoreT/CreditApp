<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
        <item name="android:colorBackground">@color/background</item>
    </style>

    <style name="AcLaunch" parent="AppTheme">
        <item name="android:background">@color/colorPrimaryDark</item>
    </style>

    <style name="AcLaunch.TextView" parent="AppTheme"></style>

    <style name="AcLaunch.TextView.Wellcome" parent="AcLaunch.TextView">
        <item name="android:textSize">64sp</item>
    </style>

    <style name="AcLaunch.TextView.Register" parent="AcLaunch.TextView">
        <item name="android:textSize">16sp</item>
    </style>

    <style name="EditText" parent="AppTheme">
        <item name="android:gravity">center</item>
        <item name="android:textSize">26sp</item>
    </style>

    <style name="EditText.Email"></style>
    <style name="EditText.Password"></style>

    <style name="Button"></style>
    <style name="Button.SignIn" parent="Button"></style>





   <!-- <style name="AppTheme.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="AppTheme.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />


    <style name="AppTheme.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>-->

</resources>
